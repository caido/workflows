            "alias": "code",
            "value": {
              "data": "export function run(input, sdk) {\n  let parsed = sdk.asString(input);\r\n  var result = '';\r\n  var chars = parsed.match(/\\\\u[\\dA-F]{4}/gi);\r\n  if (chars) {\r\n    for (var i = 0; i < chars.length; i++) {\r\n      result += String.fromCharCode(parseInt(chars[i].replace(\"\\\\u\", \"\"), 16));\r\n    }\r\n  }\r\n\n  sdk.console.log(parsed);\n  return result;\n}\n",
              "kind": "string"
            }
          }
